{
  "name": "testng-results-handler-action",
  "version": "1.0.0",
  "private": true,
  "description": "Handle the results of TestNG",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "bugs": {
      "url": "https://github.com/UWHealth/testng-results-handler/issues"
    },
  "homepage": "https://github.com/UWHealth/testng-results-handler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UWHealth/testng-results-handler.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "UWHealth",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/webhooks": "^7.11.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.6.0",
    "@typescript-eslint/parser": "^3.9.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.0",
    "jest-circus": "^26.4.0",
    "js-yaml": "^3.14.0",
    "nock": "^13.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}
