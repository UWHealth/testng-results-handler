{
  "name": "testng-results-handler-action",
  "version": "1.0.1",
  "private": true,
  "description": "Handle the results of TestNG",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build lib/main.js",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "bugs": {
    "url": "https://github.com/UWHealth/testng-results-handler/issues"
  },
  "homepage": "https://github.com/UWHealth/testng-results-handler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UWHealth/testng-results-handler.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "UWHealth",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/webhooks": "^12.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/parser": "^5.60.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.43.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "js-yaml": "^4.1.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
